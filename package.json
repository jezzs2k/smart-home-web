{
  "name": "smart-home-web",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview --port 8080",
    "update_redux": "yarn delete_redux_global && cp -rf ./src/stores ../redux/stores",
    "copy_redux": "yarn delete_redux && cp -rf ../redux/stores ./src/stores",
    "delete_redux": "rm -rf ./src/stores",
    "delete_redux_global": "rm -rf ../redux/stores"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@reduxjs/toolkit": "1.6.2",
    "antd": "4.16.13",
    "axios": "0.24.0",
    "bootstrap": "5.1.3",
    "chart.js": "3.6.0",
    "date-fns": "2.26.0",
    "firebase": "9.5.0",
    "lodash": "4.17.21",
    "react": "^17.0.0",
    "react-bootstrap": "2.0.2",
    "react-chartjs-2": "3.3.0",
    "react-dom": "17.0.0",
    "react-redux": "7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "4.1.2",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react": "^1.0.0",
    "typescript": "4.3.2",
    "vite": "2.6.4"
  }
}
